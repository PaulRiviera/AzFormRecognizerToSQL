<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>AzFormRecognizer.Table.ToSQL</PackageId>
    <Version>0.1.2</Version>
    <Authors>Paul Riviera</Authors>
    <PackageDescription>Convert Azure Form Recognizer Table output to T-SQL commands</PackageDescription>
    <RepositoryUrl>https://github.com/PaulRiviera/AzFormRecognizerToSQL</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Models\" />
    <None Remove="Azure.AI.FormRecognizer" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Models\" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Azure.AI.FormRecognizer" Version="4.0.0" />
  </ItemGroup>
</Project>
